(require 'f)

(defvar composable.el-support-path
  (f-dirname load-file-name))

(defvar composable.el-features-path
  (f-parent composable.el-support-path))

(defvar composable.el-root-path
  (f-parent composable.el-features-path))

(add-to-list 'load-path composable.el-root-path)

(require 'composable.el)
(require 'espuds)
(require 'ert)

(Setup
 ;; Before anything has run
 )

(Before
 ;; Before each scenario is run
 )

(After
 ;; After each scenario is run
 )

(Teardown
 ;; After when everything has been run
 )
